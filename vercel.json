{
  "builds": [
    {
      "src": "api/arxiv.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/arxiv",
      "dest": "api/arxiv.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
